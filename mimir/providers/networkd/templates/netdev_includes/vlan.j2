Id={{ interface.id }}
{%- if interface.parameters.protocol %}
Protocol={{ interface.parameters.protocol }}
{%- endif %}
{%- if interface.parameters.gvrp != None %}
GVRP={{ interface.parameters.gvrp | to_systemd_bool }}
{%- endif %}
{%- if interface.parameters.mvrp != None %}
MVRP={{ interface.parameters.mvrp | to_systemd_bool }}
{%- endif %}
{% if interface.parameters.loose_binding != None %}
LooseBinding={{ interface.parameters.loose_binding }}
{%- endif %}
{%- if interface.parameters.reorder_header %}
ReorderHeader={{ interface.parameters.reorder_header }}
{%- endif -%}
